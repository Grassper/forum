{
  "expo": {
    "name": "eforum",
    "slug": "eforum",
    "version": "1.0.0",
    "orientation": "portrait",

    "privacy": "hidden",
    "icon": "./assets/appIcons/drawable-hdpi/logo.png",
    "splash": {
      "image": "./assets/appIcons/ios/splash_screen@1x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/appIcons/drawable-hdpi/splash_screen.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "com.nodeberry.eforum",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.nodeberry.eforum",
      "versionCode": 1,
      "splash": {
        "ldpi": "./assets/appIcons/drawable-ldpi/splash_screen.png",
        "hdpi": "./assets/appIcons/drawable-hdpi/splash_screen.png",
        "mdpi": "./assets/appIcons/drawable-mdpi/splash_screen.png",
        "xdpi": "./assets/appIcons/drawable-xdpi/splash_screen.png",
        "xxdpi": "./assets/appIcons/drawable-xxdpi/splash_screen.png",
        "xxhdpi": "./assets/appIcons/drawable-xxhdpi/splash_screen.png",
        "xxxhdpi": "./assets/appIcons/drawable-xxxhdpi/splash_screen.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcons/drawable-ldpi/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/appIcons/drawable-ldpi/logo.png"
    },
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
