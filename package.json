{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\" --config ./.prettierrc.js",
    "compile": "tsc --watch",
    "typecheck": "tsc -p tsconfig.json",
    "precommit": "lint-staged && pretty-quick --staged"
  },
  "dependencies": {
    "expo": "~42.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^27.0.2",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "babel-loader": "^8.2.2",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-native-wcandillon": "^3.7.1",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "^42.1.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.0.0"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
