{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "expo_install": "expo install",
    "eject": "expo eject",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "compile": "tsc --watch",
    "typecheck": "tsc -p tsconfig.json",
    "precommit": "lint-staged && pretty-quick --staged"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/netinfo": "6.0.2",
    "@react-native-masked-view/masked-view": "0.2.5",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "aws-amplify": "^4.3.3",
    "aws-amplify-react-native": "^5.0.4",
    "expo": "^43.0.0",
    "expo-app-loading": "~1.2.1",
    "expo-font": "~10.0.3",
    "expo-status-bar": "~1.1.0",
    "native-base": "^3.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "react-native-config": "^1.4.4",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-pager-view": "5.4.6",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-server": "0.0.1-beta",
    "react-native-svg": "12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-web": "0.17.1",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^17.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "^43.0.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.3.5"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
