    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react-devtools": "^4.21.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.3.5"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "name": "app",
  "version": "1.0.0"
}
